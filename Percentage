        // Percentage calculation;
        #include<stdio.h>
        int main (){
        int m1=98;  //maths marks
        int m2=96;  //chemistry marks
        int m3=99;  //physics marks
        int m4=94;  //biology marks
        int m5=96;  //english marks
        int percentage=(m1+m2+m3+m4+m5)/5;
        printf("The percentage obtained by student is %d",percentage);
        return 0;
        }