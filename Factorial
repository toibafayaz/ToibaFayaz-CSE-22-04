        // Factorial of number using function :
        #include<stdio.h>
        int factorial (int);
        int main(){
          int x;
          printf("Enter a number :");
          scanf("%d",&x);
          int fact=factorial(x);
          printf("The factorial of number is %d ",fact);
          
           return 0;
          }
          int factorial(int n){
           int multiply=1;
           for(int i=1;i<=n;i++)
           {
           multiply=multiply*i;
           }
           return multiply;
           }