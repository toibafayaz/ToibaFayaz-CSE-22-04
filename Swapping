        // Swapping of numbers :
        #include<stdio.h>
        void swap (int*,int*);
        int main(){
          int a,b;
          printf("Enter first number :");
          scanf("%d",&a);
          printf("Enter second number :");
          scanf("%d",&b);
          swap(&a,&b);
          printf("After swapping : Value of a is %d and value of b is %d",a,b);
          return 0;
          }
           void swap(int* x,int* y){
           int temp=*x;
           *x=*y;
           *y=temp;
            
            }