      //Uppercase and lowercase letters:
      #include<stdio.h>
      int main (){
      //Uppercase:
      for(int i=65;i<=90;i++)
        {
        printf("%d ",i);
        char ch =(char)i;
        printf("%c\n",ch);
        }
        //Lowercase:
        for(int i=97;i<=122;i++)
        {
        printf("%d ",i);
        char ch =(char)i;
        printf("%c \n",ch);
        }
        return 0;
        }